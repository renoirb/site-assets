								<w:spacing w:after="0" w:line="240" w:line-rule="auto"/>
								<w:ind w:left="337" w:hanging="180"/>
							</w:pPr>
							<w:r wsp:rsidRPr="00826FD0">
								<w:t>Created a client-side DataTable rendering system with Vanilla JavaScript</w:t>
