									<w:i/>
								</w:rPr>
								<w:t>Senior Technical Consultant (Lead Front-End)</w:t>
							</w:r>
						</w:p>
						
						<w:p wsp:rsidR="00294F25" wsp:rsidRPr="00826FD0" wsp:rsidRDefault="00294F25" wsp:rsidP="00826FD0">
